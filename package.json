{
	"name": "vscode-recessstyles",
	"displayName": "Recess Styles CSS intellisense",
	"description": "Recess Styles CSS class name completion",
	"publisher": "jmenestr",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Intellisense"
	],
	"activationEvents": [
		"onLanguage:typescriptreact"
	],
	"main": "./out/extension.js",
	"contributes": {
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/color": "^3.0.0",
		"@types/css": "^0.0.31",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"css": "^2.2.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"color": "^3.1.1"
	}
}
